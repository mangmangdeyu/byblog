    private void rightRotate() {
        //1 创建一个新的节点，值等于当前节点的值
        Node newRight = new Node(value);
        //2 把新节点的右子树设置成当前节点的右子树
        newRight.right=right;
        //3 把新节点的左子树设置为当前节点的左子树右子树
        newRight.left=left.right;
        //4 把当前节点的值设置为左子节点的值
        value=left.value;
        //5 把当前节点的左子树设置左子树的左子树
        left=left.left;
        //6 把当前节点的右子树设置为新节点
        right=newRight;

    }
